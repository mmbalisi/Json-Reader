        <Label x:Name="lblShipId" Content="Label" HorizontalAlignment="Left" Height="44" Margin="10,48,0,0" VerticalAlignment="Top" Width="418" FontFamily="NSimSun" FontSize="14"/>
        <Label x:Name="lblShipRegistry" Content="Label" HorizontalAlignment="Left" Height="44" Margin="10,97,0,0" VerticalAlignment="Top" Width="418" FontFamily="NSimSun" FontSize="14"/>
        <Label x:Name="lblShipName" Content="Label" HorizontalAlignment="Left" Height="44" Margin="10,146,0,0" VerticalAlignment="Top" Width="418" FontFamily="NSimSun" FontSize="14"/>
        <Label x:Name="lblShipClass" Content="Label" HorizontalAlignment="Left" Height="44" Margin="10,195,0,0" VerticalAlignment="Top" Width="418" FontFamily="NSimSun" FontSize="14"/>
        <Label x:Name="lblShipService" Content="Label" HorizontalAlignment="Left" Height="44" Margin="10,244,0,0" VerticalAlignment="Top" Width="418" FontFamily="NSimSun" FontSize="14"/>

    </Grid>
